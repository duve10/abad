RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.php -f
RewriteRule ^(.*)$ $1.php
RewriteRule ^calificacion/([0-9]+) calificacion.php?id=$1
RewriteRule ^calificacion/([0-9]+)/([0-9a-zA-Z_-]+) calificacion.php?id=$1&descripcion=$2
RewriteRule ^nivel1/([0-9]+) nivel1.php?id=$1
RewriteRule ^nivel1/([0-9]+)/([0-9a-zA-Z_-]+) nivel1.php?id=$1&descripcion=$2